		<!-- 操作类型定义 -->
		<sqlMap resource="ibatis/Sql_MbnSmsOperationClass.xml"/>
		<!-- 短信回复编码表 -->
		<sqlMap resource="ibatis/Sql_MbnSmsMerchantOperationTaskNumber.xml"/>
		<!-- 彩信操作 -->
		<sqlMap resource="ibatis/sqlmap_mms.xml"/>
