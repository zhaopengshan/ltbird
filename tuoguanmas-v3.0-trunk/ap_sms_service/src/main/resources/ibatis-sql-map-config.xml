		<sqlMap resource="sqlmap/SmsInboxDbIntf.xml" />
		<sqlMap resource="sqlmap/SmsOutboxDbIntf.xml" />
		<sqlMap resource="sqlmap/SmsSentDbIntf.xml" />
		
